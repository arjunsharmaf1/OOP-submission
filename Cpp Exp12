// Experiment 12 
#include <iostream>
#include <stack>
using namespace std;

int main() {
    stack<string> cars;

    cars.push("BMW");
    cars.push("Audi");
    cars.push("Mercedes");
    cars.push("Benz");

    cout << "Top element is: " << cars.top() << endl;
    cout << "Size of stack is: " << cars.size() << endl;

    cars.pop();
    cars.pop();

    while (!cars.empty()) {
        cout << "Element in stack: " << cars.top() << endl;
        cars.pop();
    }

    return 0;
}
// 2nd program

#include <iostream>
#include <queue>
using namespace std;

int main() {
    queue<int> q;

    q.push(10);
    q.push(20);
    q.push(30);
    q.push(40);
    q.push(50);

    cout << "Front element is: " << q.front() << endl;
    cout << "Back element is: " << q.back() << endl;

    q.pop();

    while (!q.empty()) {
        cout << "Element in the queue is: " << q.front() << endl;
        q.pop();
    }

    return 0;
}
